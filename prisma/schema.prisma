// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id

  // Relations
  scamguild   ScamGuild[]
}


model ScamGuild {
  id String @id

  reason    String
  credits   String[]
  admins    User[]
  invites   String[] @unique
  guildType String
}
